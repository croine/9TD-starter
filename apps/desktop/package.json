{
  "name": "9td-desktop",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"vite\" \"wait-on tcp:5173 && nodemon --watch electron --exec 'node electron/main.js'\"",
    "electron:build": "vite build && electron-builder",
    "postinstall": "shx cp -n .env.example .env || true"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-toast": "^1.1.5",
    "@react-three/drei": "^9.88.16",
    "@react-three/fiber": "^8.15.15",
    "@vitejs/plugin-react": "^5.0.4",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.441.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.160.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "electron": "^38.3.0",
    "electron-builder": "^24.9.1",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.38",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.4.5",
    "vite": "^7.1.10",
    "wait-on": "^7.2.0"
  }
}
